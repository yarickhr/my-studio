s/Videos.jsx';\nimport Shows from './Shows/Shows.jsx';\nimport Promocodes from './Promocodes/Promocodes.jsx';\nimport Campaigns from './Campaigns/Campaigns.jsx';\nimport SidebarMenu from '../components/SidebarMenu/SidebarMenu.jsx';\nimport PromocodeIcon from '../components/Icons/PromocodeIcon.jsx';\n\nimport HeaderLayout from '../layouts/HeaderLayout/HeaderLayout.jsx';\n\nimport './App.less';\n\nconst ROUTES = [\n    {\n        title: 'Videos',\n        path: '/videos',\n        icon: <PromocodeIcon/>,\n        page: <Videos/>\n    },\n    {\n        title: 'Shows',\n        path: '/shows',\n        icon: <PromocodeIcon/>,\n        page:  <Shows/>,\n        editableSubmenu: ['Nike Air Max', 'Nike Sport']\n    },\n    {\n        title: 'Promocode',\n        path: '/promocode',\n        icon: <PromocodeIcon/>,\n        page:  <Promocodes/>,\n    },\n    {\n        title: 'Campaigns',\n        path: '/campaigns',\n        icon: <PromocodeIcon/>,\n        page:  <Campaigns/>\n    }\n];\n\nclass App extends Component {\n    render() {\n        return (\n            <div className='app'>\n                <SidebarMenu menu={ROUTES}/>\n                <Switch>\n                    {ROUTES.map(({path, page}) => (\n                        <Route \n                            key={path}\n                            path={path}>\n                            <HeaderLayout>\n                                {page}\n                            </HeaderLayout>\n                        </Route>\n                    ))}\n                    <Route\n                        key='404'\n                        path=\"*\">\n                        404\n                    </Route>\n                </Switch>\n            </div>\n        )\n    }\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"f42dc5eed2d3792d4b572e589b6efe59","cacheData":{"env":{}}}