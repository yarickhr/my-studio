nk.jsx","original":{"line":41,"column":15}}],"sources":{"../components/SidebarMenu/SidebarMenuLink/SidebarMenuLink.jsx":"import React from 'react';\nimport {Link} from 'react-router-dom';\n\nimport './SidebarMenuLink.less';\n\nconst SidebarMenuLink = ({title, path, icon, editableSubmenu, mixCls, active}) => {\n    const isSumbenu = Array.isArray(editableSubmenu) && editableSubmenu.length;\n\n    const cls = `${mixCls ? mixCls : ''} ${isSumbenu ? 'link__submenu' : ''}`;\n\n    return (\n        <li \n            className={`link ${cls}`}\n            key={title}>\n            <Link \n                className={`link__title ${active ? 'link__title--active' : ''}`}\n                to={path}>\n                {icon}\n                <span className='link__title-text'>{title}</span>\n            </Link>\n            {\n               isSumbenu &&\n                <ul>\n                    {\n                        editableSubmenu.map(title => (\n                            <li \n                                className='link__submenu-item'\n                                key={title}>\n                                <Link to={`${path}/${title}`}>\n                                    {title}\n                                </Link>\n                            </li>\n                        ))\n                    }\n                </ul>\n            }\n        </li>\n    );\n}\n\nexport default SidebarMenuLink;\n"},"lineCount":null}},"error":null,"hash":"f4cb96ce92ce07c651bf66ee0b7c939b","cacheData":{"env":{}}}